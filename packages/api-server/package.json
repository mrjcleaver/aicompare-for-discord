{
  "name": "@aicompare/api-server",
  "version": "1.0.0",
  "description": "API server for AI model comparison",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seed.ts"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/websocket": "^8.3.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^8.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bullmq": "^4.15.2",
    "zod": "^3.22.4",
    "bcrypt": "^5.1.1",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.7.1",
    "@google-ai/generativelanguage": "^2.5.0",
    "cohere-ai": "^7.7.5"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0"
  }
}